<%= render :partial => "shared/user_header", :locals => {:user => @user} %>

<div class="module">
<h3>Tweets</h3>
	<% if @user.tweets.count == 0 %>
		<p> <%= @user.full_name %> has no Tweets</p>
	<% else %>
		<%= render :partial => "shared/tweet", :locals => {:tweets => @tweets}  %>
	<% end %>
</div>
<div id="infinite-scrolling">
  <%= will_paginate @tweets %>
</div>