<!DOCTYPE html>
<html>
<head>
  <title>Flutter</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
	<%= render partial: "shared/navbar" %>
<% end %>
<div class="wrapper">
	<div class="dashboard">
	<%= yield :dashboard %>
	</div>
	<div class="content-main">
		<%= yield %>
	</div>
</div>
</body>
</html>
