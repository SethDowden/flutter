<%= render :partial => "shared/dashboard_user_header", :locals => {:user => current_user} %>

<div class="module">
	<h3>Followers</h3>
	<div>
		<% if current_user.followers.count == 0 %>
			<p> No one Cares about you!</p>
		<% else %>
			<% current_user.followers.each do |user| %>
				<%= render :partial => "shared/user", :locals => {:user => user}  %>
			<% end %>
		<% end %>
	</div>
</div>